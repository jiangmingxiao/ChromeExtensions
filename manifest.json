{
    "name": "hsy_developer",
    "description": "好生意开发插件2.0",
    "version": "2.0",
    "manifest_version": 2,
    "icons": {
        "16": "img/favicon.png",
        "48": "img/favicon.png",
        "128": "img/favicon.png"
    },
    "browser_action": {
        "default_icon": "img/favicon.png",
        "default_title": "hsy_developer",
        "default_popup": "popup.html"
    },
    "commands": {
        "test": {
            "suggested_key": {
                "default": "Alt+4"
            },
            "description": "test"
        }
    },
    "permissions": [
        "declarativeContent",
        "tabs",
        "activeTab",
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ]
    }
}